          h3(class="text-justify:" style="margin-left:200px;margin-right:200px;")="Archivos:"
          ul(class="list-unstyled" style="margin-left:200px;margin-right:200px;")
            li()
              a(href="#" style='color:blue;text-decoration: underline; ')="Dist_Muest.pdf"
            li()
              a(href="#" style='color:blue;text-decoration: underline;')="Presentación_CP.pdf"

          h3(class="text-justify:" style="margin-left:200px;margin-right:200px;")="Etiquetas:"
          ul(class="list-unstyled list-inline" style="margin-left:200px;margin-right:200px;" )
            li()
              a(href="#" class="tag")="Estadística"
            li()
              a(href="#" class="tag")="Matemáticas"
